#include<iostream>
using namespace std;
int main() {
    int num;
    cin>>num;
    int esum = 0;
    int osum = 0;
    while(num>0) {
        int digit = num % 10;
        if(digit%2==0) {
            esum = esum + digit;
        }
        else {
            osum = osum + digit;
        }
        num/=10;
    }
    cout<<"Even count : "<<esum<<endl;
    cout<<"Odd count : "<<osum;
    return 0;
}
