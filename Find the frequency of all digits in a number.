#include<iostream>
using namespace std;
int main() {
    int num;
    cin>>num;
    int digit,i;
    int count[10] = {0};
    while(num>0) {
        digit = num % 10;
        count[digit]++;
        num/=10;
    }
    for(i=0;i<10;i++) {
        if(count[i] > 0) {
            cout<<"Digit "<<i <<" : "<<count[i]<<endl;
        }
    }
    return 0;
}
