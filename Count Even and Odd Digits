#include<iostream>
using namespace std;
int main() {
    int num;
    cin>>num;
    int ecount = 0;
    int ocount = 0;
    while(num>0) {
        int digit = num % 10;
        if(digit%2==0) {
            ecount++;
        }
        else {
            ocount++;
        }
        num/=10;
    }
    cout<<"Even count : "<<ecount<<endl;
    cout<<"Odd count : "<<ocount;
    return 0;
}
