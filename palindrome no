#include<iostream>
using namespace std;
int main() {
    int num;
    cin>>num;
    int digit;
    int rev=0;
    int temp = num;
    while(num>0) {
        digit = num % 10;
        rev = rev*10 + digit;
        num/=10;
    }
    if(rev==temp) {
        cout<<"Palindrome number";
    }
    else {
        cout<<"Not a Palindrome number";
    }
    return 0;
}
